<h1>Wikinimous</h1>
<%= link_to "New Article", new_article_path, :class => "new-article" %>

<ul>
  <% @articles.each do |article| %>
    <li>
      <div class="title"><%= link_to article.title, article_path(article) %></div>
      <div class="description">&emsp;<%= article.content %></div>
      <br>
    </li>
  <% end %>
</ul>
